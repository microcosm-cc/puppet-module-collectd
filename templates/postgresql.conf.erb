# Generated by Puppet
LoadPlugin "postgresql"

<Plugin "postgresql">

<% @queries.each do |qname,qvalue| -%>
  <Query "<%= qname %>">
  	Statement "<%=qvalue['statement']%>"
    <Result>
    </Result>
  </Query>
<% end -%>

<% @databases.each do |dname,dvalue| -%>
  <Database "<%= dname %>">
  	Host "<%=dvalue['host']%>"
  	Port "<%=dvalue['port']%>"
  	User "<%=dvalue['user']%>"
  	Password "<%=dvalue['password']%>"
  </Database>
<% end -%>

</Plugin>